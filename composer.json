{
    "name": "slaxweb/router",
    "description": "Router component for the SlaxWeb/Framework",
    "license": "MIT",
    "version": "0.3.0",
    "authors": [
        {
            "name": "Tomaz Lovrec",
            "email": "tomaz.lovrec@gmail.com"
        }
    ],
    "require": {
		"php":">=7.0.0",
        "slaxweb/hooks": "0.4.*@dev",
        "slaxweb/logger": "0.2.*@dev",
        "slaxweb/config": "0.1.*@dev",
        "symfony/http-foundation": "~3.0"
    },
    "require-dev": {
        "codeception/codeception": "dev-master",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "behat/gherkin": "~4.4"
    },
    "suggest": {
        "pimple/pimple": "Make use of available providers for Pimple version 3.0.x"
    },
	"autoload":	{
		"psr-4": {
			"SlaxWeb\\Router\\": "src/",
            "SlaxWeb\\Router\\Tests\\Unit\\": "tests/unit/"
		}
	},
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    }
}
